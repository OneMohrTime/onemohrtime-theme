{% if post.personality_traits %}

<dl class="c-definitionList -personalityTypes">
	{% for test in post.get_field('personality_traits') %}
		<div>

			<dt class="c-definitionList__header">{{ test.test_name }}</dt>
			<dd class="c-definitionList__data _wysiwyg">
				{% if test.answer_link %}
					<a href="{{ test.answer_link }}" target="_blank" rel="noopener">{{ test.answer }}</a>
				{% else %}
					{{ test.answer }}
				{% endif %}
			</dd>

		</div>
	{% endfor %}
</dl>

{% endif %}
